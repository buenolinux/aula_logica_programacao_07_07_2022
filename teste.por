programa
{
	
// matrizes 
// primeiro para a linha 
// segundo para a coluna
inclua biblioteca Util --> u
	inteiro local = u.sorteia(1,30) 

	inteiro vet[50], i
	funcao inicio()
	{
	
	
		para(i=0; i <51; i++)
		{
			se(i % 10 ==0 )
			{
				escreva("\n")	
			}
			senao
			{
				escreva(" * ")	
			}
		}	
		
	}
}
